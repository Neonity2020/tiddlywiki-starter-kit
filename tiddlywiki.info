{
  "description": "A modern style and elegant notebook built with Tiddlywiki5 and Tailwindcss",
  "themes": [
    "tiddlywiki/vanilla",
    "nico/notebook"
  ],
  "includeWikis": [
    {
      "read-only": false,
      "path": "src"
    }
  ],
  "plugins": [
    "tiddlywiki/filesystem",
    "tiddlywiki/tiddlyweb",
    "tiddlywiki/highlight",
    "tiddlywiki/browser-sniff",
    "tiddlywiki/pluginlibrary",
    "oeyoews/neotw-cmp",
    "oeyoews/neotw-vtable",
    "oeyoews/vue-zhipu",
    "oeyoews/neotw-filetree",
    "oeyoews/neotw-ai",
    "oeyoews/neotw-read-files",
    "oeyoews/element-plus",
    "oeyoews/vue-contextmenu",
    "oeyoews/vue-context-menu",
    "oeyoews/vue-kanban",
    "oeyoews/vue-tabs",
    "oeyoews/neotw-sitemap",
    "oeyoews/vue-rss",
    "oeyoews/neotw-vue-codeblock",
    "oeyoews/notivue",
    "oeyoews/neotw-vue-netease-banner",
    "oeyoews/neotw-vue-journal",
    "oeyoews/neotw-vue-plugins",
    "oeyoews/neotw-vue-fetch",
    "oeyoews/vue-info",
    "oeyoews/vue-draggable-plus",
    "oeyoews/vue-random-cards",
    "oeyoews/neotw-dropdown-better",
    "oeyoews/tiddler-fullscreen",
    "oeyoews/neotw-vue-todo",
    "oeyoews/vue-links-gallery",
    "oeyoews/vue-i18n",
    "oeyoews/mermaid-widget",
    "oeyoews/mermaid930",
    "oeyoews/markdown-it-mermaid",
    "oeyoews/markdown-it-math",
    "oeyoews/markdown-it-pangu",
    "oeyoews/medium-zoom",
    "oeyoews/blockquote",
    "oeyoews/neotw-draft-better",
    "oeyoews/image-observer",
    "oeyoews/markdown-it-emoji",
    "oeyoews/markdown-it-abbr",
    "oeyoews/markdown-it-front-matter",
    "oeyoews/markdown-it-github-alert",
    "oeyoews/markdown-extensions-startup",
    "oeyoews/skill-badges",
    "oeyoews/notebook-theme-sidebar-resizer",
    "oeyoews/font-geist",
    "oeyoews/qrcode",
    "oeyoews/notify",
    "oeyoews/neotw-spotlight",
    "oeyoews/neotw-image-gallery",
    "oeyoews/tiddlywiki-motion",
    "oeyoews/neotw-pwa",
    "oeyoews/neotw-zen-mode",
    "oeyoews/commandpalette",
    "oeyoews/neotw-icons",
    "oeyoews/neotw-copy-code2",
    "oeyoews/tiddlywiki-daylight",
    "oeyoews/tiddlywiki-videos",
    "oeyoews/neotw",
    "oeyoews/echarts-addons",
    "oeyoews/nprogress",
    "oeyoews/tiddlywiki-modal-ui",
    "oeyoews/tiddlywiki-tailwindcss-plus",
    "oeyoews/neotw-notion-gallery",
    "oeyoews/tiddlywiki-tiddler-info",
    "oeyoews/tiddlywiki-gravatar",
    "oeyoews/neotw-homepage",
    "tiddlywiki/markdown",
    "oeyoews/markdown-kit",
    "oeyoews/tiddlywiki-publish-tiddler",
    "oeyoews/neotw-vue3"
  ],
  "build": {
    "javascript": [
      "--output",
      ".tiddlywiki",
      "--render",
      ".",
      "javascript.json",
      "text/plain",
      "$:/core/templates/exporters/JsonFile",
      "exportFilter",
      "[tag[JavaScript]]"
    ],
    "journal": [
      "--output",
      ".tiddlywiki",
      "--render",
      ".",
      "journal.json",
      "text/plain",
      "$:/core/templates/exporters/JsonFile",
      "exportFilter",
      "[tag[Journal]type[text/markdown]!sort[created]]"
    ],
    "sitemap": [
      "--output",
      ".tiddlywiki",
      "--render",
      "$:/plugins/oeyoews/neotw-sitemap/sitemap",
      "sitemap.xml",
      "text/plain"
    ],
    "tiddlers": [
      "--render",
      ".",
      "tiddlers.json",
      "text/plain",
      "$:/core/templates/exporters/JsonFile",
      "exportFilter",
      "[publish[blog]]"
    ],
    "index": [
      "--verbose",
      "--output",
      ".tiddlywiki",
      "--deletetiddlers",
      "$:/StoryList",
      "--render",
      "$:/plugins/tiddlywiki/tiddlyweb/save/offline",
      "offline.html",
      "text/plain",
      "--render",
      "$:/plugins/tiddlywiki/tiddlyweb/save/offline",
      "tiddlers.json",
      "text/plain",
      "--render",
      "$:/core/save/offline-external-js",
      "index.html",
      "text/plain",
      "--render",
      "$:/core/templates/tiddlywiki5.js",
      "[[tiddlywikicore-]addsuffix<version>addsuffix[.js]]",
      "text/plain",
      "--savetiddler",
      "$:/favicon.ico",
      "favicon.ico",
      "--savetiddler",
      "$:/manifest.json",
      "manifest.json",
      "--savetiddler",
      "$:/tiddlywiki.png",
      "tiddlywiki.png",
      "--deletetiddlers",
      "$:/StoryList"
    ],
    "library": [
      "--output",
      ".tiddlywiki/library",
      "--makelibrary",
      "$:/UpgradeLibrary",
      "--savelibrarytiddlers",
      "$:/UpgradeLibrary",
      "[prefix[$:/plugins/oeyoews/]]",
      "recipes/library/tiddlers/",
      "$:/UpgradeLibrary/List",
      "--savetiddler",
      "$:/UpgradeLibrary/List",
      "recipes/library/tiddlers.json",
      "--rendertiddler",
      "$:/plugins/tiddlywiki/pluginlibrary/library.template.html",
      "index.html",
      "text/plain",
      "--deletetiddlers",
      "[[$:/UpgradeLibrary]] [[$:/UpgradeLibrary/List]]"
    ]
  },
  "config": {
    "retain-original-tiddler-path": false,
    "default-tiddler-location": "src/tiddlers"
  }
}