{
  "description": "A modern style and elegant notebook built with Tiddlywiki5 and Tailwindcss",
  "themes": [
    "tiddlywiki/vanilla",
    "nico/notebook"
  ],
  "includeWikis": [
    {
      "read-only": false,
      "path": "src"
    }
  ],
  "plugins": [
    "tiddlywiki/filesystem",
    "tiddlywiki/tiddlyweb",
    "tiddlywiki/highlight",
    "tiddlywiki/browser-sniff",
    "tiddlywiki/pluginlibrary",
    "oeyoews/markdown-it-twemoji",
    "oeyoews/mermaid-tiny",
    "oeyoews/tiddlywiki-tid2png",
    "oeyoews/tiddler-fullscreen",
    "oeyoews/mermaid-widget",
    "oeyoews/mermaid930",
    "oeyoews/markdown-it-mermaid",
    "oeyoews/markdown-it-math",
    "oeyoews/tiddlywiki-full-text-search",
    "oeyoews/medium-zoom",
    "oeyoews/blockquote",
    "oeyoews/editor-preview-resizer",
    "oeyoews/confetti-background",
    "oeyoews/plum-blosssom",
    "oeyoews/confetti",
    "oeyoews/image-observer",
    "oeyoews/markdown-it-emoji",
    "oeyoews/markdown-it-pangu",
    "oeyoews/markdown-it-abbr",
    "oeyoews/markdown-it-front-matter",
    "oeyoews/markdown-it-github-alert",
    "oeyoews/markdown-it-smartarrows",
    "oeyoews/markdown-extensions-startup",
    "oeyoews/translate",
    "oeyoews/pangu",
    "oeyoews/ejs",
    "oeyoews/skill-badges",
    "oeyoews/notebook-theme-sidebar-resizer",
    "oeyoews/neotw-music-with-howler",
    "oeyoews/tiddlywiki-banners",
    "oeyoews/font-geist",
    "oeyoews/neotw-fetch",
    "oeyoews/qrcode",
    "oeyoews/hitokoto",
    "oeyoews/neotw-image-better",
    "oeyoews/notify",
    "oeyoews/NPL",
    "oeyoews/neotw-spotlight",
    "oeyoews/neotw-image-gallery",
    "oeyoews/neotw-links-gallery",
    "oeyoews/tiddlywiki-motion",
    "oeyoews/neotw-pwa",
    "oeyoews/neotw-zen-mode",
    "oeyoews/commandpalette",
    "oeyoews/neotw-icons",
    "oeyoews/neotw-copy-code",
    "oeyoews/tiddlywiki-daylight",
    "oeyoews/tiddlywiki-videos",
    "oeyoews/neotw",
    "oeyoews/echarts-addons",
    "oeyoews/neotw-info",
    "oeyoews/nprogress",
    "oeyoews/tiddlywiki-modal-ui",
    "oeyoews/tiddlywiki-tailwindcss-plus",
    "oeyoews/neotw-notion-gallery",
    "oeyoews/tiddlywiki-tiddler-info",
    "oeyoews/tiddlywiki-gravatar",
    "oeyoews/neotw-homepage",
    "tiddlywiki/markdown",
    "oeyoews/markdown-kit",
    "oeyoews/tiddlywiki-publish-tiddler"
  ],
  "build": {
    "reactMd": [
      "--verbose",
      "--output",
      ".tiddlywiki/markdown/React",
      "--render",
      "[tag[React]]",
      "[addsuffix[.md]]",
      "text/plain",
      "$:/plugins/cdaven/markdown-export/md-tiddler"
    ],
    "jsMd": [
      "--verbose",
      "--output",
      ".tiddlywiki/markdown/JavaScript",
      "--render",
      "[tag[JavaScript]]",
      "[addsuffix[.md]]",
      "text/plain",
      "$:/plugins/cdaven/markdown-export/md-tiddler"
    ],
    "starMd": [
      "--verbose",
      "--output",
      ".tiddlywiki/markdown/star",
      "--render",
      "[tag[剪藏]]",
      "[addsuffix[.md]]",
      "text/plain",
      "$:/plugins/cdaven/markdown-export/md-tiddler"
    ],
    "journalMd": [
      "--verbose",
      "--output",
      ".tiddlywiki/markdown/Journal",
      "--render",
      "[tag[Journal]]",
      "[addsuffix[.md]]",
      "text/plain",
      "$:/plugins/cdaven/markdown-export/md-tiddler"
    ],
    "javascript": [
      "--output",
      ".tiddlywiki",
      "--render",
      ".",
      "javascript.json",
      "text/plain",
      "$:/core/templates/exporters/JsonFile",
      "exportFilter",
      "[tag[JavaScript]]"
    ],
    "journal": [
      "--output",
      ".tiddlywiki",
      "--render",
      ".",
      "journal.json",
      "text/plain",
      "$:/core/templates/exporters/JsonFile",
      "exportFilter",
      "[tag[Journal]type[text/markdown]!sort[created]]"
    ],
    "tiddlers": [
      "--render",
      ".",
      "tiddlers.json",
      "text/plain",
      "$:/core/templates/exporters/JsonFile",
      "exportFilter",
      "[publish[blog]]"
    ],
    "index": [
      "--verbose",
      "--output",
      ".tiddlywiki",
      "--deletetiddlers",
      "$:/StoryList",
      "--render",
      "$:/plugins/tiddlywiki/tiddlyweb/save/offline",
      "offline.html",
      "text/plain",
      "--render",
      "$:/plugins/tiddlywiki/tiddlyweb/save/offline",
      "tiddlers.json",
      "text/plain",
      "--render",
      "$:/core/save/offline-external-js",
      "index.html",
      "text/plain",
      "--render",
      "$:/core/templates/tiddlywiki5.js",
      "[[tiddlywikicore-]addsuffix<version>addsuffix[.js]]",
      "text/plain",
      "--savetiddler",
      "$:/favicon.ico",
      "favicon.ico",
      "--savetiddler",
      "$:/manifest.json",
      "manifest.json",
      "--savetiddler",
      "$:/tiddlywiki.png",
      "tiddlywiki.png",
      "--deletetiddlers",
      "$:/StoryList"
    ],
    "library": [
      "--output",
      ".tiddlywiki/library",
      "--makelibrary",
      "$:/UpgradeLibrary",
      "--savelibrarytiddlers",
      "$:/UpgradeLibrary",
      "[prefix[$:/plugins/oeyoews/]]",
      "recipes/library/tiddlers/",
      "$:/UpgradeLibrary/List",
      "--savetiddler",
      "$:/UpgradeLibrary/List",
      "recipes/library/tiddlers.json",
      "--rendertiddler",
      "$:/plugins/tiddlywiki/pluginlibrary/library.template.html",
      "index.html",
      "text/plain",
      "--deletetiddlers",
      "[[$:/UpgradeLibrary]] [[$:/UpgradeLibrary/List]]"
    ]
  },
  "config": {
    "retain-original-tiddler-path": false,
    "default-tiddler-location": "src/tiddlers"
  }
}